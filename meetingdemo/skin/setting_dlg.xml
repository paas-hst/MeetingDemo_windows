          <Slider name="slider_aud" height="14" width="300" padding="0,13,0,13" thumbsize="14,14" bkimage="img\\slidebar_bg.png" foreimage="img\\slidebar_fore.png" thumbimage="img\\slidebar_thumb.png" />
